<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple_500">

    <RelativeLayout
        android:id="@+id/R"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        tools:ignore="MissingConstraints">


        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/R123"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="90dp"
            android:gravity="center"
            android:layout_marginBottom="-122dp"
            android:text=" Choisir la place exacte"
            android:textColor="#FCC64A"
            android:textSize="24sp"
            android:textStyle="bold"/>

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:contentDescription="@string/todo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/table" />


        <RelativeLayout
            android:id="@+id/R84"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="170dp"
            android:layout_marginBottom="170dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="@+id/imageView2"
            app:layout_constraintVertical_bias="0.362"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="-16dp">

            <Button
                android:id="@+id/place4"
                android:layout_width="38dp"
                android:layout_height="47dp"
                android:layout_marginEnd="203dp"
                android:layout_marginRight="203dp"
                android:layout_toStartOf="@+id/place8"
                android:layout_toLeftOf="@+id/place8"
                android:background="#ffffff"
                android:text="4"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="348dp"
                tools:layout_editor_absoluteY="342dp" />

            <Button
                android:id="@+id/place8"
                android:layout_width="38dp"
                android:layout_height="47dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="19dp"
                android:layout_marginRight="19dp"
                android:background="#ffffff"
                android:text="8"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="342dp" />

            <RelativeLayout
                android:id="@+id/R567"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="@+id/imageView2"
                app:layout_constraintTop_toTopOf="@+id/imageView2"
                app:layout_constraintVertical_bias="0.362"
                tools:ignore="MissingConstraints,NotSibling"
                tools:layout_editor_absoluteX="-16dp">

                <Button
                    android:id="@+id/place5"
                    android:layout_width="38dp"
                    android:layout_height="47dp"


                    android:layout_marginEnd="25dp"
                    android:layout_marginRight="25dp"
                    android:layout_toStartOf="@+id/place6"
                    android:layout_toLeftOf="@+id/place6"
                    android:background="@color/purple_700"
                    android:text="5"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="261dp"
                    tools:layout_editor_absoluteY="435dp" />

                <Button
                    android:id="@+id/place6"
                    android:layout_width="38dp"
                    android:layout_height="47dp"
                    android:layout_marginEnd="30dp"
                    android:layout_marginRight="30dp"
                    android:layout_toStartOf="@+id/place7"
                    android:layout_toLeftOf="@+id/place7"
                    android:background="@color/black"
                    android:text="6"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="186dp"
                    tools:layout_editor_absoluteY="435dp" />

                <Button
                    android:id="@+id/place7"
                    android:layout_width="38dp"
                    android:layout_height="47dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="70dp"
                    android:layout_marginRight="112dp"
                    android:background="@color/black"
                    android:text="7"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="115dp"
                    tools:layout_editor_absoluteY="435dp" />

            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/R123"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="250dp"
            app:layout_constraintBottom_toBottomOf="@+id/title"
            app:layout_constraintTop_toTopOf="@+id/title"
            app:layout_constraintVertical_bias="0.62"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="-16dp">


            <Button
                android:id="@+id/place2"
                android:layout_width="38dp"
                android:layout_height="47dp"
                android:layout_marginEnd="25dp"
                android:layout_marginRight="25dp"
                android:layout_toStartOf="@+id/place3"
                android:layout_toLeftOf="@+id/place3"
                android:background="@color/black"
                android:text="2"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="186dp"
                tools:layout_editor_absoluteY="244dp" />

            <Button
                android:id="@+id/place3"
                android:layout_width="38dp"
                android:layout_height="47dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="70dp"
                android:layout_marginRight="112dp"
                android:background="@color/black"
                android:text="3"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="261dp"
                tools:layout_editor_absoluteY="244dp" />

            <Button
                android:id="@+id/place1"
                android:layout_width="38dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="24dp"

                android:layout_toStartOf="@+id/place2"
                android:layout_toLeftOf="@+id/place2"
                android:background="@color/black"
                android:text="1"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="115dp"
                tools:layout_editor_absoluteY="244dp" />


        </RelativeLayout>


    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>